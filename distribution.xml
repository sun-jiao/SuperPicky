<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>慧眼选鸟 SuperPicky</title>
    <organization>com.jamesphotography</organization>
    <domains enable_localSystem="true"/>
    <options customize="never" require-scripts="false" hostArchitectures="arm64,x86_64"/>

    <welcome file="welcome.html" mime-type="text/html"/>
    <license file="LICENSE.txt" mime-type="text/plain"/>
    <conclusion file="conclusion.html" mime-type="text/html"/>

    <choices-outline>
        <line choice="default">
            <line choice="com.jamesphotography.superpicky"/>
        </line>
    </choices-outline>

    <choice id="default"/>
    <choice id="com.jamesphotography.superpicky" visible="false">
        <pkg-ref id="com.jamesphotography.superpicky"/>
    </choice>

    <pkg-ref id="com.jamesphotography.superpicky" version="4.0.3" onConclusion="none">
        SuperPicky-component.pkg
    </pkg-ref>
</installer-gui-script>
